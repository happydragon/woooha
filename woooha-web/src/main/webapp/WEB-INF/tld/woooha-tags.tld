<?xml version="1.0" encoding="UTF-8"?>
<taglib version="2.0" xmlns="http://java.sun.com/xml/ns/j2ee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/j2ee http://java.sun.com/xml/ns/j2ee/web-jsptaglibrary_2_0.xsd">
	<display-name>"Woooha Tags"</display-name>
	<tlib-version>1.0</tlib-version>
	<short-name>woooha</short-name>
	<uri>/woooha-tags</uri>
	<tag>
        <!-- remove me! -->
		<name>ScoreStar</name>
		<tag-class>com.woooha.web.tag.StarTag</tag-class>
		<body-content>empty</body-content>
	</tag>
	<tag>
		<name>Static</name>
		<tag-class>com.woooha.web.tag.StaticTag</tag-class>
		<body-content>empty</body-content>
        <attribute>
            <description><![CDATA[resource type]]></description>
            <name>type</name>
            <required>false</required>
            <rtexprvalue>false</rtexprvalue>
        </attribute>
        <attribute>
            <description><![CDATA[Which resource to be retrieved]]></description>
            <name>resource</name>
            <required>false</required>
            <rtexprvalue>false</rtexprvalue>
        </attribute>
        <attribute>
            <description><![CDATA[Whether output base64-coded result]]></description>
            <name>outputBase64</name>
            <required>false</required>
            <rtexprvalue>false</rtexprvalue>
        </attribute>
	</tag>
</taglib>